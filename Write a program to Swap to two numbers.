#include <iostream>
using namespace std;

int main()
{
    int a = 58, b = 92, temp;

    cout << "Before swapping." << endl;
    cout << "a = " << a << ", b = " << b << endl;

    temp = a;
    a = b;
    b = temp;

    cout << "\nAfter swapping." << endl;
    cout << "a = " << a << ", b = " << b << endl;

    return 0;
}
Output

Before swapping.
a = 58, b = 92

After swapping.
a = 92, b = 58
